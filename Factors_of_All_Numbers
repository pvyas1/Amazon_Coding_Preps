
public class Factors_of_All_Numbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 102;
		
		System.out.println("The Factors of num "+num+" are: ");
		for(int i = 1; i <= num/2; i++){
			if(num%i == 0)
				System.out.print(i+",");
		}
		System.out.print(num);
	}
}
